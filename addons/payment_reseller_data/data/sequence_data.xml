<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
  		<record id="inv_seq" model="ir.sequence">
           <field name="name">Invoice Seq</field>
           <field name="code">inv_num</field>
           <field name="prefix">INV</field>
           <field eval="1" name="number_next"/>
           <field eval="1" name="number_increment"/>
           <field eval="6" name="padding"/>
  		</record>
  		
  		<record id="or_seq" model="ir.sequence">
           <field name="name">Receipt Seq</field>
           <field name="code">or_code</field>
           <field name="prefix">OR</field>
           <field eval="1" name="number_next"/>
           <field eval="1" name="number_increment"/>
           <field eval="6" name="padding"/>
  		</record>
  		
  		<record id="exp_seq" model="ir.sequence">
           <field name="name">Expense Seq</field>
           <field name="code">exp_code</field>
           <field name="prefix">EXP</field>
           <field eval="1" name="number_next"/>
           <field eval="1" name="number_increment"/>
           <field eval="6" name="padding"/>
  		</record>
  		
  		<record id="base.main_company" model="res.company">
            <field name="name">Fashion Centre</field>
            <field name="abbreviation">FC</field>
            <field name="rml_header1">...go on trends!</field>
            <field name="currency_id" ref="base.PHP"/>
            <field name="logo" type="base64" file="payment_reseller_data/static/src/img/fc_logo.png"/>
            <!-- <field name="partner_id" ref="base.main_partner"/> -->
        </record>
  	</data>
  </openerp>