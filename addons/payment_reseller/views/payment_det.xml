<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="podet_tree_view">
			<field name="name">payment_det.tree</field>
			<field name="model">payment_reseller.payment_det</field>
			<field name="arch" type="xml">
				<tree string="Payment Detail" editable="bottom">
					<field name="po_det_id"></field>	
				</tree>
			</field>
		</record>
		
	
		<record model="ir.ui.view" id="podet_form_view">
			<field name="name">payment_det.form</field>
			<field name="model">payment_reseller.payment_det</field>
			<field name="arch" type="xml">
				<form string="Payment Detail">
					<sheet>
						<group>
								<field name="po_det_id"></field>
						</group>
					</sheet>
				</form>
			</field>
		</record>
	
	</data>
</openerp>